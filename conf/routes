
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
# GET     /                           controllers.HomeController.index
GET     /                           controllers.Assets.at(path="/public", file="index.html")
# An example controller showing how to use dependency injection

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /test                       controllers.LocalController.testFetch

POST    /setApiKey                  controllers.LocalController.setApiKey

POST    /localLogin                 controllers.LocalController.localLogin

POST    /localLogout                controllers.LocalController.localLogout

POST    /importObservers            controllers.LocalController.importObservers

POST    /addOneObserver             controllers.LocalController.addOneObserver

POST    /removeOneObserver          controllers.LocalController.removeOneObserver

POST    /updateOneObserver          controllers.LocalController.updateOneObserver


POST    /queryAllUser               controllers.RemoteController.queryAllUser

POST    /queryUserApiKey            controllers.RemoteController.queryUserApiKey

POST    /queryUserObserver          controllers.RemoteController.queryUserObserver

POST    /queryObserverMonitor       controllers.RemoteController.queryObserverMonitor

POST    /login                      controllers.RemoteController.login

POST    /logout                     controllers.RemoteController.logout

POST    /addUser                    controllers.RemoteController.addUser

POST    /updateUserPassword         controllers.RemoteController.updateUserPassword

POST    /updateUserApiLimit         controllers.RemoteController.updateUserApiLimit

POST    /updateSelfPassword         controllers.RemoteController.updateSelfPassword

POST    /addSelfApiKey              controllers.RemoteController.addSelfApiKey

POST    /updateSelfApiKey           controllers.RemoteController.updateSelfApiKey

POST    /fetchAllObservers          controllers.RemoteController.fetchAllObservers

POST    /addOneObserver             controllers.RemoteController.addOneObserver

POST    /removeOneObserver          controllers.RemoteController.removeOneObserver

POST    /updateOneObserver          controllers.RemoteController.updateOneObserver

POST    /addAllObservers            controllers.RemoteController.addAllObservers
